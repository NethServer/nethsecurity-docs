msgstr ""

#: ../../objects.rst:82 ../../objects.rst:122 84ae99ceb73b4578b581ef2ec2e173e0
#: 8dd4ee4d28514e0a8d8cdf3e90955667
msgid "Use cases:"
msgstr ""

#: ../../objects.rst:84 f2946cfe2bfd48fc965aa8f2b8db8939
msgid "restrict OpenVPN users to specific network resources"
msgstr ""

#: ../../objects.rst:85 506304e1a7a14322bc957737b6f9317a
msgid "create user-based allow/deny lists"
msgstr ""

#: ../../objects.rst:86 c347bc36a73546979a1ac27622f2799e
msgid "implement time-based access policies for remote users"
msgstr ""

#: ../../objects.rst:87 58ccdab828cf4a42bd041aff036565bc
msgid "monitor and control per-user bandwidth usage"
msgstr ""

#: ../../objects.rst:89 3f5a237ed7c046f4a0cd9d115f3a4486
msgid "Requirements:"
msgstr ""

#: ../../objects.rst:91 ae4b9bb196474562bec18af47a636bb0
msgid "user has OpenVPN access enabled"
